type float64

entity Movimiento {
 	numero  				: string
 	estado_movimiento		: string
 	fecha_movimiento		: string
 	numero_oficio			: integer
 	fecha_oficio			: string
 	descripcion				: string
 	unidad_ejecutora		: integer
 	rubro_origen		: string
 	rubro_destino		: string
 	valor					      : integer
 	tipo_movimiento			: string
  apropiacion: Apropiacion
}

entity Apropiacion {
	vigencia: integer
  mes: integer
	valor_inicial: integer
  rubro: Rubro
}

entity Rubro {
	codigo: string
	Nombre: string
	entidad: string
	descripcion: string
	unidad_ejecutora: integer
	hijos: string
}

entity DisponibilidadApropiacion {
  valor: integer
  fuente_financiamiento: integer
  apropiacion: Apropiacion
}

entity RegistroPresupuestal {
  vigencia: integer
  fecha_registro: string
  estado: string
  numero_registro_presupuestal: integer
  solicitud: integer
  disponibilidad_apropiacion: DisponibilidadApropiacion
}

entity OrdenPago {
  vigencia: integer
  valor_base: integer
  unidad_ejecutora: integer
  forma_pago: integer
  registro_presupuestal: RegistroPresupuestal
}
