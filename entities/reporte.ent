type float64

entity Movimiento {
 	numero  				: string
 	estado_movimiento		: string
 	fecha_movimiento		: string
 	numero_oficio			: integer
 	fecha_oficio			: string
 	descripcion				: string
 	unidad_ejecutora		: integer
 	apropiacion_destino		: integer
 	apropiacion_origen		: integer
 	valor					      : integer
 	tipo_movimiento			: string
}

entity Apropiacion {
	vigencia: integer
  mes: integer
  mes: integer
	valor_inicial: integer
	movimiento: Movimiento
}

entity Rubro {
	codigo: string
	Nombre: string
	entidad: string
	descripcion: string
	unidad_ejecutora: integer
	apropiacion: Apropiacion
}

entity DisponibilidadApropiacion {
  apropiacion: Apropiacion
  valor: integer
  fuente_financiamiento: integer
}

entity RegistroPresupuestal {
  disponibilidad_apropiacion: DisponibilidadApropiacion
  valor: integer
}
